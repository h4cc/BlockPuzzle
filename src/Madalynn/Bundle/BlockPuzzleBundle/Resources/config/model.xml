<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="block_puzzle.repository.level" class="Madalynn\Bundle\BlockPuzzleBundle\Repository\LevelRepository" factory-service="doctrine.odm.mongodb.document_manager" factory-method="getRepository">
            <argument type="string">Madalynn\Bundle\BlockPuzzleBundle\Document\Level</argument>
        </service>
    </services>
</container>